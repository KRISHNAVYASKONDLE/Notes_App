<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Storedata</servlet-name>
  	<display-name>Storedata</display-name>
  	<description></description>
  	<servlet-class>com.notemu.Storedata</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Filedata</servlet-name>
  	<display-name>Filedata</display-name>
  	<description></description>
  <servlet-class>com.notemu.Filedata</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Storedata</servlet-name>
  	<url-pattern>/Storedata</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Filedata</servlet-name>
  	<url-pattern>/Filedata</url-pattern>
  </servlet-mapping>

  
  
</web-app>
